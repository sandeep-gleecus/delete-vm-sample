    <script type="text/javascript">
        //Needed because ScriptManager tries to check dependencies before the main scripts are loaded
        if (!window) this.window = this;
        window.Type = Function;
        Type._checkDependency = function (a, b) { return true; }

        //Set the current timezone if available
        var SpiraContext = {};
        SpiraContext.BaseUrl = '<%=GetBaseUrl()%>';
        SpiraContext.BaseThemeUrl = '<%=GetThemeUrl()%>';
        SpiraContext.ThemeName = '<%=Page.Theme%>';
        SpiraContext.CurrentTimezone = '<%=GetTimezone() %>';
        SpiraContext.CurrentUsername = '<%=GetUserName() %>';
        SpiraContext.CurrentUserId = '<%=GetUserId() %>';
        SpiraContext.IsSystemAdmin = <%=IsSystemAdmin.ToString().ToLowerInvariant() %>;
		SpiraContext.IsReportAdmin = <%=IsReportAdmin.ToString().ToLowerInvariant() %>;
        SpiraContext.IsPortfolioAdmin = <%=IsPortfolioAdmin.ToString().ToLowerInvariant() %>;
        SpiraContext.IsGroupAdmin = <%=GetIsGroupAdmin()%>;
        SpiraContext.IsMobile = <%=isMobileDevice()%>;
        SpiraContext.IsProjectAdmin = <%=GetIsProjectAdmin()%>;
        SpiraContext.IsTemplateAdmin = <%=GetIsTemplateAdmin()%>;
        SpiraContext.ProductType = '<%=GetProductType()%>';
        SpiraContext.ProjectRoleId = '<%=GetProjectRoleId()%>';
        SpiraContext.ProjectId = <%=GetProjectId()%>;
        SpiraContext.ProjectGroupId = <%=GetProjectGroupId()%>;
        SpiraContext.ProjectTemplateId = <%=GetProjectTemplateId()%>;
        SpiraContext.PortfolioId = <%=GetPortfolioId()%>;
        SpiraContext.WorkspaceType = <%=GetWorkspaceType()%>;
        SpiraContext.CanUserAddCommentToArtifacts = <%=CanUserAddCommentToArtifacts.ToString().ToLowerInvariant()%>;
        SpiraContext.ProjectRole = <%=ProjectRole%>;
        SpiraContext.Permissions = <%=Permissions%>;
        SpiraContext.IsEmailEnabled = <%=Inflectra.SpiraTest.Common.ConfigurationSettings.Default.EmailSettings_Enabled.ToString().ToLowerInvariant()%>;
        SpiraContext.uiState = {};
        SpiraContext.AntiXsrfToken = <%=GetAntiXsrfTokenValue()%>;

        SpiraContext.WorkspaceEnums = {
            product: 1,
            program: 2,
            template: 3,
            portfolio: 4,
            enterprise: 5,
            release: 0, // used by gantt charts on dashboards
            releaseTask: -2, // used by gantt charts on dashboards
            sprint: -1, // used by gantt charts on dashboards
            sprintTask: -3 // used by gantt charts on dashboards
        };

        SpiraContext.Features = {
            baselines: <%=Inflectra.SpiraTest.Common.Global.Feature_Baselines.ToString().ToLowerInvariant()%>,
            portfolios: <%=Inflectra.SpiraTest.Common.Global.Feature_Portfolios.ToString().ToLowerInvariant()%>,
            risks: <%=Inflectra.SpiraTest.Common.Global.Feature_Risks.ToString().ToLowerInvariant()%>,
        }
    </script>
