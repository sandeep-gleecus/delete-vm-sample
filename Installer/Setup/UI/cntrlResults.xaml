<UserControl x:Class="Inflectra.SpiraTest.Installer.UI.cntrlResults"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:resx="clr-namespace:Inflectra.SpiraTest.Installer.Themes.Inflectra"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300"
			 Padding="7"
			 Background="Transparent">
	<DockPanel LastChildFill="True">
		<StackPanel Orientation="Vertical">
			<TextBlock Name="txbFinishStatus" TextWrapping="Wrap" FontWeight="Bold" FontSize="14" Text="{x:Static resx:Resources.Results_Status}" />
			<TextBlock Name="txbFinishMessage" TextWrapping="Wrap" Text="{x:Static resx:Resources.Results_Message}" />
			<Separator Margin="0,5,0,5"  />
		</StackPanel>
	</DockPanel>
</UserControl>
