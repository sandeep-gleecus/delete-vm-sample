<script type="text/javascript">
    var resx = Inflectra.SpiraTest.Web.GlobalResources;

    Mousetrap.bind('t a r d i s', function () {
        $('body').addClass('animated tada');
        $('body').one('webkitAnimationEnd mozAnimationEnd MSAnimationEnd oanimationend animationend', function() {
            $('body').removeClass('animated tada');
        });
    });
    //NAVIGATING DATAGRIDS ON LIST PAGES (KEYBOARD)
    //Toggle being able to traverse the data grid
    Mousetrap.bind('shift+x', function focusOnGrid() {
        if($('.main-content > .DataGrid').length) {
            $('.main-content > .DataGrid > tbody tr').removeClass('is-keys-active');
            $('.main-content > .DataGrid > tbody tr:first-child').addClass('is-keys-active');
            $('.main-content > .DataGrid').toggleClass('is-keys-active');
            pageMasterGrid.rowCount = $('.main-content .DataGrid > tbody tr:not(.Pagination)').length;
            //change setting to store whether the grid is in use or not (to help with unbinding and binding)
            pageMasterGrid.gridIsActive = !pageMasterGrid.gridIsActive;
            pageMasterGrid.setSelectedRow();
            //bind and unbind depending on state 
            if(pageMasterGrid.gridIsActive) {
                pageMasterGrid.unbind();
                pageMasterGrid.bind();
            } else {
                pageMasterGrid.unbind();
                pageMasterDetails.bind();
            }
        }
    });
    //Toggle the selector Checkbox on the currently selected row
    Mousetrap.bind('x', function toggleCheck() {
        var checkboxRow = $('.DataGrid.is-keys-active tr.is-keys-active'),
            checkbox = $('.DataGrid.is-keys-active tr.is-keys-active .Checkbox input');
        checkbox.prop("checked", !checkbox.prop("checked"));
        checkboxRow.toggleClass('Highlighted');
        checkboxRow.toggleClass('Normal');
    });
    
    //Go to the artifact listed in the row (in current tab, in new tab)
    Mousetrap.bind('enter', function openItem() {
        var rowTarget = $('.DataGrid.is-keys-active tr.is-keys-active td.priority1 a').attr('href');
        if(rowTarget) {
            window.location = $('.DataGrid.is-keys-active tr.is-keys-active td.priority1 a').attr('href');
        }
    });
    Mousetrap.bind('shift+enter', function openItemNewWindow() {
        var rowTarget = $('.DataGrid.is-keys-active tr.is-keys-active td.priority1 a').attr('href');
        if(rowTarget) {
            window.open($('.DataGrid.is-keys-active tr.is-keys-active td.priority1 a').attr('href'), '_blank');
        }
    });
    pageMasterGrid = {
        gridIsActive: false,
        rowCount: '',
        currentRow: '',
        setSelectedRow: function () {
            $('.DataGrid tr td:first-child').removeClass('shadow-l-orange-dark');
            $('.DataGrid.is-keys-active tr.is-keys-active td:first-child').addClass('shadow-l-orange-dark');
            pageMasterGrid.currentRow = $('.DataGrid.is-keys-active tr.is-keys-active').index();
        },
        bind: function() {
           //Move up or down a row
            Mousetrap.bind('j', function moveRowDown() {
                if (pageMasterGrid.rowCount > (pageMasterGrid.currentRow + 1)) {
                    $('.DataGrid.is-keys-active tr.is-keys-active').removeClass('is-keys-active').next().addClass('is-keys-active');
                    pageMasterGrid.setSelectedRow();
                }
            });
            Mousetrap.bind('k', function moveRowUp() {
                if (pageMasterGrid.currentRow > 0) {
                    $('.DataGrid.is-keys-active tr.is-keys-active').removeClass('is-keys-active').prev().addClass('is-keys-active');
                    pageMasterGrid.setSelectedRow();
                }
            }); 
        },
        unbind: function() {
            Mousetrap.unbind('j');
            Mousetrap.unbind('k');
        }
    };

	window.addEventListener("load", function () {
        pageMasterDetails.bind();

        //If in debug mode
        //if (document.getElementById("<%=ltrDebug.ClientID%>")) {
            // change the nav bg color
        //    document.documentElement.style.setProperty('--nav-bg', 'rgba(119,52,57,.9)');
        //}
    });

    //NAVIGATING DETAILS PAGES (KEYBOARD)
    //Move right or left one tab
    var pageMasterDetails = {
        bind: function() {
            //only bind on a standard page
            if( $('.main-content').length ) {
                Mousetrap.bind('l', function moveTabRight() {
                    var tabCount = $('.main-content .TabControl > .Tab').length,
                        currentTab = $('.main-content .TabControl > .Tab.TabSelected').index();
                    if (tabCount > (currentTab + 1)) {
                        $('.main-content .TabControl > .Tab.TabSelected').next().click();
                    }
                });
                Mousetrap.bind('h', function moveTabLeft() {
                    var currentTab = $('.main-content .TabControl > .Tab.TabSelected').index();
                    if (currentTab > 0) {
                        $('.main-content .TabControl > .Tab.TabSelected').prev().click();
                    }
                });
            }
        }
    }

    
</script>
</html>
